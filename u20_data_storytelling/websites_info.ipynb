    "# use regex\n",
    "pattern = re.compile(r\"(?<=\\d\\.)(.*)\")\n",
    "breeds = [\n",
    "    pattern.search(breed).group().strip()\n",
    "    for breed in breed_names\n",
    "    if pattern.search(breed)\n",
    "]\n",
    "sorted(breeds)\n",
